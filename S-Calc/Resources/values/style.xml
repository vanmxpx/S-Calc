<?xml version="1.0" encoding="utf-8" ?>
<resources>
  <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimary">#F5F5F5</item>
    <!--<item name="colorAccent">#FF4081</item>-->
    <item name="drawerArrowStyle">@style/MainArrowStyle</item>
  </style>
  <style name="MainArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
    <item name="color">#1C1C1C</item>
    <item name="spinBars">true</item>
  </style>
  <style name="grey_keyboard">
    <item name="android:keyTextSize">30dp</item>
    <item name="android:fontFamily">roboto</item>
    <item name="android:keyBackground">@drawable/keybackground</item>
    <item name="android:keyTextColor">@android:color/white</item>
  </style>
</resources>